	<xs:complexType name="DerivativeTransactionObject">

		<xs:complexContent>

			<xs:extension base="SecurityTransactionObject">

				<xs:sequence>

					<xs:element name="hedgeTrade" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Used to specify whether the derivatives order is Hedge, Trade , Covered or Uncovered
</xs:documentation>
