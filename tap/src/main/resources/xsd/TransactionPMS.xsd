								<xs:enumeration value="2"/>

								<xs:enumeration value="3"/>

								<xs:enumeration value="4"/>

							</xs:restriction>

						</xs:simpleType>

					</xs:element>

					<xs:element name="splitPortfolio" minOccurs="0">
