</xs:documentation>
							<xs:appinfo>Entity : ext_operation - Attribute :  op_exch_rate - Datatype : exchange_t</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="transactionCurrency" type="infra:Identifier" minOccurs="0">
